<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WANT</servlet-name>
		<servlet-class>org.app.want.rex.WANTServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WANT</servlet-name>
		<url-pattern>/want</url-pattern>
	</servlet-mapping>
<!-- 	parasite servlets	 -->
	<servlet>
		<servlet-name>ParasiteUserAccount</servlet-name>
		<servlet-class>org.app.want.parasite.servlets.UserAccountServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParasiteUserAccount</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ParasiteFiles</servlet-name>
		<servlet-class>org.app.want.parasite.servlets.FilesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParasiteFiles</servlet-name>
		<url-pattern>/files</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
